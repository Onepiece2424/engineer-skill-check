<div class="article_form">

  <%= form_with model: article, local: true do |f| %>
    <div class="form_group">
      <%= f.label :title, "タイトル" %>
      <%= f.text_field :title %>
    </div>

    <div class="form_group">
      <%= f.text_field :content %>
    </div>

    <%= f.submit '保存' %>
  <% end %>
</div>
